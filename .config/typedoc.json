{
	"$schema": "https://typedoc.org/schema.json",
	"out": "../dist",
	"name": "@stephansama actions",
	"readme": "../README.md",
	"favicon": "https://raw.githubusercontent.com/stephansama/static/refs/heads/main/favicon-actions.svg",
	"customCss": "../node_modules/@stephansama/catppuccin-typedoc/dist/catppuccin-typedoc.css",
	"navigation": true,
	"excludeExternals": true,
	"skipErrorChecking": true,
	"jsDocCompatibility": true,
	"darkHighlightTheme": "catppuccin-mocha",
	"lightHighlightTheme": "catppuccin-latte",
	"entryPointStrategy": "packages",
	"entryPoints": ["../*"],
	"exclude": ["**/ISSUE_TEMPLATE/**"],
	"navigationLinks": {
		"Author": "https://stephansama.info",
		"Blog": "https://madprofessorblog.org",
		"Actions": "https://github.com/stephansama/actions",
		"Packages": "https://github.com/stephansama/packages"
	},
	"highlightLanguages": [
		"bash",
		"console",
		"css",
		"html",
		"javascript",
		"json",
		"jsonc",
		"json5",
		"yaml",
		"tsx",
		"typescript",
		"markdown"
	]
}

{
	"$schema": "https://typedoc.org/schema.json",
	"out": "./dist",
	"name": "@stephansama actions",
	"readme": "./README.md",
	"favicon": "https://api.iconify.design/octicon:github-action.svg",
	"customCss": "https://unpkg.com/@stephansama/catppuccin-typedoc@1.0.0/dist/catppuccin-typedoc.css",
	"navigation": true,
	"excludeExternals": true,
	"skipErrorChecking": true,
	"jsDocCompatibility": true,
	"darkHighlightTheme": "catppuccin-mocha",
	"lightHighlightTheme": "catppuccin-latte",
	"entryPointStrategy": "packages",
	"entryPoints": ["./*"],
	"exclude": ["**/ISSUE_TEMPLATE/**"],
	"navigationLinks": {
		"Author": "https://stephansama.info",
		"Blog": "https://madprofessorblog.org",
		"GitHub": "https://github.com/stephansama/actions"
	},
	"highlightLanguages": [
		"bash",
		"console",
		"css",
		"html",
		"javascript",
		"json",
		"jsonc",
		"json5",
		"yaml",
		"tsx",
		"typescript",
		"markdown"
	]
}

{
	"semi": true,
	"useTabs": true,
	"quoteProps": "consistent",
	"singleQuote": false,
	"overrides": [
		{ "files": ["*.md"], "options": { "tabWidth": 2, "useTabs": false } }
	],
	"plugins": [
		"prettier-plugin-jsdoc",
		"prettier-plugin-organize-imports",
		"prettier-plugin-sort-package-json"
	]
}
